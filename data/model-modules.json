      "_moduleOutputKey": "net_to_equity"
    },
    {
      "id": 9052,
      "groupId": 55,
      "name": "Distributions: DSCR Test",
      "formula": "MAX((R9071 >= C1.47), (ABS(R9068) < 0.001)) * F2",
