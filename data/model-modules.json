      "_moduleId": "M3",
      "_moduleOutputKey": "gst_received"
    },
    {
      "id": 9011,
      "groupId": 51,
      "name": "GST Paid/Received: GST Receivable - Closing",
      "formula": "CUMSUM(R9007) - CUMSUM(R9010)",
      "type": "stock",
      "_moduleId": "M3",
      "_moduleOutputKey": "receivable_closing"
    },
    {
