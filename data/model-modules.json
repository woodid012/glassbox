    {
      "id": 9032,
      "groupId": 54,
      "name": "MRA (Maintenance Reserve): Opening Balance",
      "formula": "MAX(0, (CUMSUM(R9033) - R9033) - (CUMSUM(R9034) - R9034) - (CUMSUM(R9035) - R9035))",
