      "_moduleId": "M8",
      "_moduleOutputKey": "commitment_fee"
    },
    {
      "id": 9082,
      "groupId": 57,
      "name": "Total DSRF Fees",
      "formula": "R9080 + R9081 + M8.2",
      "type": "flow",
      "_moduleId": "M8",
      "_moduleOutputKey": "total_dsrf_fees"
    },
    {
      "id": 9083,
